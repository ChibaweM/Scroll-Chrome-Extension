{
  "name": "Starlabs Rapid7 Investigations Auto-Scroll ",
  "description": "Starlabs tool to help Auto-Scroll investigations page for easier and faster SLA calculation",
  "version": "0.1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_title": "Scroll to the bottom"
  },
  "background":{
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
